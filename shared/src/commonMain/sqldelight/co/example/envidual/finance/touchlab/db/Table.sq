CREATE TABLE stockdata (
id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
ticker TEXT NOT NULL
);

selectAll:
SELECT * FROM stockdata;

selectById:
SELECT * FROM stockdata WHERE id = ?;

selectByName:
SELECT * FROM stockdata WHERE ticker = ?;

insertStock:
INSERT OR REPLACE INTO stockdata(id, ticker)
VALUES (?,?);

deleteAll:
DELETE FROM stockdata;
